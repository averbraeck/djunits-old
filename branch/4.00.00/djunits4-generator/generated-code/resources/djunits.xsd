    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="FORCEUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(N|dyn|kgf|ozf|lbf|tnf|sn)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="PRESSUREUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(Pa|hPa|kPa|atm|Torr|at|Ba|bar|mbar|cmHg|mmHg|ftHg|inHg|kgf/mm\^2|lbf/ft\^2|lbf/in\^2|pz)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="TORQUEUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(N\.m|m\.kgf|lbf\.ft|lbf\.in)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="POWERUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(W|kW|MW|GW|ft\.lbf/h|ft\.lbf/min|ft\.lbf/s|hp\(M\)|sn\.m/s|erg/s|fW|mW|nW|pW|PW|muW|TW)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ENERGYUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(J|kJ|MJ|ft\.lbf|in\.lbf|BTU\(ISO\)|BTU\(IT\)|cal\(IT\)|kcal|Wh|kWh|MWh|GWh|TWh|mWh|muWh|eV|meV|keV|MeV|GeV|TeV|PeV|EeV|sn\.m|erg|pWh|mueV|mJ|GJ|PJ|cal|pJ|TJ|fWh|PWh|muJ|nJ|nWh)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ELECTRICALCURRENTUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(A|nA|muA|mA|kA|statA|abA|MA)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ELECTRICALCHARGEUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(C|mAh|F|e|statC|Fr|esu|abC|emu|mC|pC|kAh|MAh|muC|nC|Ah)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ELECTRICALPOTENTIALUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(V|muV|mV|kV|MV|statV|abV|GV|nV)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ELECTRICALRESISTANCEUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(Ohm|mOhm|kOhm|MOhm|muOhm|abOhm|nOhm|GOhm|stOhm)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ABSOLUTETEMPERATUREUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(K|C|degC|F|degF|R|degR|Re|degRe)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="TEMPERATUREUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(K|C|degC|F|degF|R|degR|Re|degRe)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="LINEARDENSITYUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(/m|/mm|/cm|/dm|/dam|/hm|/km|/ft|/in|/mi|/NM|/yd|/nm|/ly|/pm|/Mm|/A|/fm|/am|/pc|/mum|/AU)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

