<?xml version="1.0" encoding="UTF-8"?> <xsd:schema targetNamespace="http://www.djunits.org/djunits"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:djunits="http://www.djunits.org/djunits"
  xmlns="http://www.djunits.org/djunits" elementFormDefault="qualified">

  <!-- ========================================================================================= -->
  <!-- ==================================== DJUNITS UNIT TYPES ================================= -->
  <!-- ========================================================================================= -->

  <xsd:simpleType name="DURATIONUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(ms|msec|s|sec|m|min|h|hr|hour|day|wk|week|as|asec|ns|nsec|mus|musec|fs|fsec|ps|psec)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="TIMEUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(ms|msec|s|sec|m|min|h|hr|hour|d|day|wk|week|mus|musec)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="LENGTHUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(m|mm|cm|dm|dam|hm|km|ft|in|mi|NM|yd|A|fm|am|Mm|AU|mum|nm|pc|ly|pm)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="POSITIONUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(m|mm|cm|dm|dam|hm|km|ft|in|mi|NM|yd|A|fm|am|Mm|AU|mum|nm|pc|ly|pm)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="SPEEDUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(m/s|km/h|mi/h|ft/s|kt|m/h|km/s|mi/min|in/min|ft/min|ft/h|mi/s|in/h|in/s)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ACCELERATIONUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(km/h\^2|m/s\^2|ft/s\^2|in/s\^2|mi/h\^2|mi/s\^2|kt/s|mi/h/s|g|Gal)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="AREAUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(m\^2|km\^2|cm\^2|mm\^2|are|a|ha|mi\^2|ft\^2|in\^2|yd\^2|acre|ac|Mm\^2|am\^2|nm\^2|mum\^2|fm\^2|dam\^2|ca|dm\^2|pm\^2|hm\^2|Nmi\^2)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="VOLUMEUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(m\^3|dm\^3|L|cm\^3|km\^3|mi\^3|ft\^3|in\^3|yd\^3|gal\(US\)|gal\(imp\)|US fl oz|fl oz \(imp\)|pt\(US fl\)|pt \(imp\)|qt\(US fl\)|qt\(imp\)|Mm\^3|am\^3|mm\^3|pc\^3|fm\^3|mum\^3|ly\^3|pm\^3|nm\^3|dam\^3|hm\^3)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ANGLEUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(rad|deg|\'|&quot;|grad|c\'|c&quot;|\%)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="DIRECTIONUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(rad\(N\)|rad\(North\)|rad|rad\(E\)|rad\(East\)|deg\(N\)|deg\(North\)|deg|deg\(E\)|deg\(East\))"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ANGLESOLIDUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(sr|sq\.deg)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="MASSUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(kg|g|lb|oz|long tn|sh tn|t|mts_t|Da|MeV|fg|Tg|EeV|mg|GeV|mug|Gg|Pg|mueV|keV|pg|ng|meV|PeV|TeV|eV|Mg)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="DENSITYUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(kg/m\^3|g/cm\^3)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="FREQUENCYUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(Hz|kHz|MHz|GHz|THz|rpm|/ms|/s|/min|/h|/day|/wk|/fs|/ns|/mus|/ps|/as)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="FLOWMASSUNITTYPE">
     <xsd:restriction base="xsd:string">
      <xsd:pattern value="(kg/s|lb/s)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="FLOWVOLUMEUNITTYPE">
     <xsd:restriction base="xsd:string">
